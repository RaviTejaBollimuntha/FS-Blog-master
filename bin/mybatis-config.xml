<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <!-- Global mapper enable caching -->
        <!--<setting name="cacheEnabled" value="true"/>-->
        <!-- When querying, turn off associated objects for instant loading to improve performance -->
        <setting name="lazyLoadingEnabled" value="true"/>
        <!-- Sets the form of the associated object load, where the field is loaded on demand(the load field is specified by SQL) and all fields of the associated table are not loaded to improve performance-->
        <setting name="aggressiveLazyLoading" value="false"/>
        <!-- For unknown SQL queries, different result sets are allowed to be returned for generic results -->
        <!--<setting name="multipleResultSetsEnabled" value="true"/>-->
        <!-- Allow use of column labels instead of column names-->
        <!--<setting name="useColumnLabel" value="true"/>-->
        <!-- Allows the use of custom primary key values (such as UUID 32-bit code generated by the program as a key value), the data table PK generation policy will be overwritten-->
        <setting name="useGeneratedKeys" value="true"/>
        <!-- Turn on the hump column name switch _ for exampleï¼šgmt_create = > gmtCreate -->
        <setting name="mapUnderscoreToCamelCase" value="true"/>
        <!-- Give nested resultMap To field-property mapping support -->
        <!--<setting name="autoMappingBehavior" value="FULL"/>-->
        <!--Cache SQL for bulk update operations to improve performance  -->
        <!--<setting name="defaultExecutorType" value="BATCH"/>-->
        <!-- The database has exceeded 25,000 seconds and has not responded to the timeout -->
        <!--<setting name="defaultStatementTimeout" value="25000"/>-->
        <!--<setting name="logImpl" value="SLF4J"/>-->
        <!-- Logging -->
        <setting name="logImpl" value="LOG4J2"/>
    </settings>
</configuration>